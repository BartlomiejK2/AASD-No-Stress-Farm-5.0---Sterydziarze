admins = [
    'admin@xmpp_server'
]
'config-type' = 'default'
debug = [ 'server' ]
'default-virtual-host' = 'xmpp_server'
dataSource () {
    default () {
        uri = 'jdbc:derby:derby;create=true'
    }
}
http () {
    setup () {
        'admin-password' = 'admin'
        'admin-user' = 'admin'
    }
}
pubsub () {
    trusted = [ 'http@{clusterNode}' ]
}
